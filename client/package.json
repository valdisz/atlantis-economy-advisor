    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "resolutions": {
    "graphql": "15.5.0"
  },
  "multipleStaticFileCopier": [
    {
      "origin": "static",
      "destination": "dist/"
    }
  ]
}
